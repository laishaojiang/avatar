#canvas {
  margin: 20rpx auto;
  border: 1rpx solid #eee;
  /* height: 250rpx;
  width: 250rpx; */
  display: none;
}

.draw-wrap {
  border: 1rpx solid #eee;
  margin-top: 20rpx;
  padding: 1rpx;
  overflow: hidden;
}

.img-avatar {
  height: 250px;
  width: 250px;
  display: flex;
  box-sizing: border-box;
  /* z-index: 5 */
}

.img-select {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 6;
}

.zIndex {
  z-index: 5
}

.primary {
  background: #317EE7;
}

.orange {
  background: #FF7F02;
}

.green {
  background: #00aa00;
}

.btns {
  /* margin-top: 40rpx;
  margin-bottom: 20rpx; */
  padding: 40rpx 20rpx 20rpx;
}

.btns .btn {
  margin: 0 10rpx;
  font-size: 28rpx;
  color: #fff;
  width: 200rpx;
  padding: 0;
  height: 80rpx;
  margin: 0 20rpx;
  flex-grow: 0
}
.tab-bar {
  height: 84rpx;
  background: #fff;
  color: #878787;
  font-size: 32rpx;
  text-align: center;
  margin-top: 10rpx;

}

.tab-bar .item {
  flex: 1;
  position: relative;
  line-height: 84rpx;


}
.tab-bar .item .dot {
  width: 24rpx;
  height: 24rpx;
  background: #ff0000;
  color: #ffffff;
  font-size: 20rpx;
  border-radius: 50%;
  position: absolute;
  right: 24rpx;
  top: 12rpx;
  line-height: 24rpx;
  text-align: center;
}


.tab-bar .active {
  color: #317ee7;
}
.tab-bar .active::after {
  content: '';
  display: block;
  position: absolute;
  width: 100rpx;
  height: 6rpx;
  bottom: 0;
  left: 50%;
  background: #317ee7;
  transform: translateX(-50rpx);
}
.tab-container {
  padding: 20rpx 30rpx;
}

.tab-c {
  flex-wrap: wrap;
}
.tab-c .active {
  background-color: #eee;
}
.imgbox {
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border: 1rpx solid #eee;
}
.imgbox image {
  width: 100%;
  height: 100%;
}
