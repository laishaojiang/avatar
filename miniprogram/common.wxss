.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 10;
}
.arrow-right {
	position: relative;
}
.arrow-right::after {
  content: "";
  position: absolute;
  top: 50%;
  right: 40rpx;
  margin-top: -6rpx;
  display: block;
  width: 12rpx;
  height: 12rpx;
  border-top: 3rpx solid #c4c4c4;
  border-right: 3rpx solid #c4c4c4;
  transform: rotate(45deg);
}
.page-wrap {
  background: #efeff4;
  min-height: 100vh;
  box-sizing: border-box;
}
